procedure prueba8 is
	type rang is new integer range 1..10;
	type vector is array (rang) of integer;
	a : vector;
	b : integer;
	idx : rang;
	i : integer;
	procedure vparam(v: out vector) is
		jdx : rang;
	begin
		jdx := 1;
		while jdx < 11 loop

			puti(v(jdx));
			new_line;
			jdx := jdx+1;
		end loop;
		
	end vparam;

begin
	idx := 0;
	i := 1;

	while idx < 11 loop
	a(idx) := i;
	i:=i+1;
	idx := idx+1;
	end loop;
	vparam(a);
	

	
end prueba8;
