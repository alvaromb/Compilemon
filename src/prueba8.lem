procedure prueba8 is

	type rang is new integer range 1..50;

	type vector2 is array (rang, rang) of integer;

	a : vector2;
	i, j:rang;
	k:integer;

begin

	i:=1;
	j:=1;
	k:=1;

	while i < 50 loop
		while j < 50 loop
			a(i,j):= k;
			j:=j+1;
		end loop;
		j:=1;
		i:=i+1;
		k:=k+1;
	end loop;
	puti(a(49,49));
end prueba8;
